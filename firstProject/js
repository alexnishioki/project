    <script>
      var canvas = document.getElementById('myCanvas');
      var context = canvas.getContext('2d');

      context.beginPath();
      context.arc(10,20,5,0,2*Math.PI);
      context.stroke()
      context.fillStyle = 'blue'
      context.fill()

      context.beginPath();
      context.arc(1250,40,4,0,2*Math.PI);
      context.stroke()
      context.fillStyle = 'red'
      context.fill()

      context.beginPath();
      context.arc(1200,150,4,0,2*Math.PI);
      context.stroke()
      context.fillStyle = 'red'
      context.fill()

      context.beginPath()
      context.moveTo(1,1)
      context.lineTo(1300,10)
      context.lineWidth = 20
      context.stroke()
      
