    <script>
      var canvas = document.getElementById('myCanvas');
      var context = canvas.getContext('2d');

      context.beginPath();
      context.arc(80,25,5,0,2*Math.PI);
      context.stroke()
      context.fillStyle = 'red'
      context.fill()

       context.beginPath();
      context.moveTo(1,1)
      context.lineTo(200,200)
      context.quadraticCurveTo(230,220,310,200)
      context.lineTo(400,200)
      context.lineTo(600,200)
      context.quadraticCurveTo(700,0,310,200)
      context.fill()
      context.stroke()
      context.beginPath()
      context.moveTo(700,200)
      context.lineTo(600,200)
      context.quadraticCurveTo(800,0,420,300)
      context.stroke()
      context.fill()
      context.beginPath()
      context.moveTo(200,1000)
      context.lineTo(800,200)
      context.lineTo(900,800)
      context.stroke()
      context.fill()
      context.beginPath()
      context.moveTo(20,600)
      context.lineTo(20,70)
      context.lineTo(350,400)
      context.lineTo(300,800)
      context.fill()
      context.stroke()
      context.beginPath()
      context.moveTo(1500,1)
      context.lineTo(1000,600)
      context.lineTo(800,1)
      context.stroke()
      context.fill()
      context.beginPath()
      context.moveTo(1300,700)
      context.lineTo(1300,600)
      context.lineTo(1200,500)
      context.quadraticCurveTo(1000,0,700,60)
      context.stroke()
      context.fill()
      context.beginPath()
      context.moveTo(600,400)
      context.lineTo(410,560)
      context.lineTo(400,400)
      context.lineTo(400,600)
      context.lineTo(600,400)
      context.fill()
      context.stroke()
      context.beginPath()
      context.moveTo(350,300)
      context.lineTo(490,400)
      context.lineWidth = 15
      context.stroke()
    </script>

